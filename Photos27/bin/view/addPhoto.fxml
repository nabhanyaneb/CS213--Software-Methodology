<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane 

	xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="view.AddPhotoController">
	
	<Label layoutX="175.0" layoutY="50.0" text="Add a photo: "/>
	<Label layoutX="30.0" layoutY="95.0" text="File: "/>
	<TextField fx:id="filename" layoutX="70" layoutY="90" prefHeight="25.0" prefWidth="300" promptText="File Name"/>
	

  	<Button fx:id="quit" layoutX="310" layoutY="5" mnemonicParsing="false" prefHeight="20" prefWidth="80" text="Quit" onAction="#createAction"/>
   	<Button fx:id="logout" layoutX="310" layoutY="30" mnemonicParsing="false" prefHeight="20" prefWidth="80" text="Logout" onAction="#createAction"/>
   	
   	<Button fx:id="back" layoutX="100" layoutY="160" mnemonicParsing="false" prefHeight="20" prefWidth="80" text="Back" onAction="#createAction"/>
   	<Button fx:id="addPhoto" layoutX="220" layoutY="160" mnemonicParsing="false" prefHeight="20" prefWidth="80" text="Add Photo" onAction="#createAction"/>
  
</AnchorPane>