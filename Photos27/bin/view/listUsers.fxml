<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane 

	xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="view.ListUsersController">
	
	<Label layoutX="160.0" layoutY="40.0" text="List of Users:"/>
	
   	<Button fx:id="quit" layoutX="310" layoutY="5" mnemonicParsing="false" prefHeight="20" prefWidth="80" text="Quit" onAction="#listAction"/>
   	<Button fx:id="logout" layoutX="310" layoutY="30" mnemonicParsing="false" prefHeight="20" prefWidth="80" text="Logout" onAction="#listAction"/>
   	
   	<TextArea fx:id="userList" layoutX="75.0" layoutY="70.0" prefHeight="80" prefWidth="250.0" promptText="User Details" />
   
   	
   	<Button fx:id="back" layoutX="140" layoutY="160" mnemonicParsing="false" prefHeight="20" prefWidth="120" text="Back" onAction="#listAction"/>
  
</AnchorPane>