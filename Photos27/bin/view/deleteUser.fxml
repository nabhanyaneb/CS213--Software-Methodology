<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane 

	xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="view.DeleteUserController">
	
	<Label layoutX="175.0" layoutY="50.0" text="Delete a User: "/>
	<Label layoutX="30.0" layoutY="95.0" text="User: "/>
	<TextField fx:id="username" layoutX="70" layoutY="90" prefHeight="25.0" prefWidth="300" promptText="Username"/>
	
  	<Button fx:id="quit" layoutX="310" layoutY="5" mnemonicParsing="false" prefHeight="20" prefWidth="80" text="Quit" onAction="#deleteAction"/>
   	<Button fx:id="logout" layoutX="310" layoutY="30" mnemonicParsing="false" prefHeight="20" prefWidth="80" text="Logout" onAction="#deleteAction"/>
   	
   	<Button fx:id="back" layoutX="100" layoutY="160" mnemonicParsing="false" prefHeight="20" prefWidth="80" text="Back" onAction="#deleteAction"/>
   	<Button fx:id="removeUser" layoutX="220" layoutY="160" mnemonicParsing="false" prefHeight="20" prefWidth="80" text="Remove" onAction="#deleteAction"/>
  
</AnchorPane>