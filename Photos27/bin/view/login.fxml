<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane 

	xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="view.LoginController">
	
	<Label layoutX="120.0" layoutY="40.0" text="Please Enter Your Username:"/>
	<Label layoutX="30.0" layoutY="75.0" text="User: "/>
	<TextField fx:id="username" layoutX="70" layoutY="70" prefHeight="25.0" prefWidth="300" promptText="Username"/>
	
	<Button fx:id="login" layoutX="150" layoutY="120" mnemonicParsing="false" prefHeight="35" prefWidth="75" text="Login" onAction="#loginAction"/>
   	<Button fx:id="quit" layoutX="310" layoutY="5" mnemonicParsing="false" prefHeight="20" prefWidth="80" text="Quit" onAction="#loginAction"/>
  
</AnchorPane>
