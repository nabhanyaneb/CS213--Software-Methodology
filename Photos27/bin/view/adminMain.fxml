<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane 

	xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="view.AdminMainController">
	
	<Label layoutX="150.0" layoutY="40.0" text="Admin Subsystem:"/>
	
   	<Button fx:id="quit" layoutX="310" layoutY="5" mnemonicParsing="false" prefHeight="20" prefWidth="80" text="Quit" onAction="#adminAction"/>
   	<Button fx:id="logout" layoutX="310" layoutY="30" mnemonicParsing="false" prefHeight="20" prefWidth="80" text="Logout" onAction="#adminAction"/>
   	
   	<Button fx:id="listUsers" layoutX="140" layoutY="70" mnemonicParsing="false" prefHeight="20" prefWidth="120" text="List Users" onAction="#adminAction"/>
   	<Button fx:id="createUser" layoutX="140" layoutY="100" mnemonicParsing="false" prefHeight="20" prefWidth="120" text="Create User" onAction="#adminAction" />
   	<Button fx:id="deleteUser" layoutX="140" layoutY="130" mnemonicParsing="false" prefHeight="20" prefWidth="120" text="Delete User" onAction="#adminAction"/>
  
</AnchorPane>
